        <Rectangle Height="214" HorizontalAlignment="Right" Margin="0,542,93,0" Name="rectangle2" Stroke="Gray" VerticalAlignment="Top" Width="343" Grid.RowSpan="2" />
        <TextBox Background="#CDB8BFB8" BorderThickness="0" FontFamily="Century Gothic" FontSize="25" Height="32" HorizontalAlignment="Right" Margin="0,0,107,33" Name="textBox15" Padding="0" VerticalAlignment="Bottom" Width="316" Text="C." Grid.Row="1" TabIndex="12" />
        <TextBox Background="#CDB8BFB8" BorderThickness="0" FontFamily="Century Gothic" FontSize="25" Height="32" HorizontalAlignment="Right" Margin="0,0,107,88" Name="textBox16" Padding="0" VerticalAlignment="Bottom" Width="316" Text="B." Grid.Row="1" TabIndex="11" />
        <TextBox Background="#CDB8BFB8" BorderThickness="0" FontFamily="Century Gothic" FontSize="25" Height="32" HorizontalAlignment="Right" Margin="0,0,107,141" Name="textBox17" Padding="0" VerticalAlignment="Bottom" Width="316" Text="A." Grid.Row="1" TabIndex="10" />
