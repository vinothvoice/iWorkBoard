        <Button Background="Azure" BorderThickness="0" Content="ERASE" FontFamily="Century Gothic" FontSize="16" Height="35" HorizontalAlignment="Center" Margin="587,0,651,52" Name="button6" VerticalAlignment="Bottom" Width="75" Click="button6_Click" Grid.Row="1" />
        <TextBox Height="32" HorizontalAlignment="Left" Margin="43,132,0,0" Name="textBox1" VerticalAlignment="Top" Width="288" Padding="0" Background="#CDB8BFB8" FontFamily="Century Gothic" FontSize="25" BorderThickness="0" Text="1." MaxLines="1" TabIndex="1" />
        <Button Background="#FFFFFFE3" BorderThickness="0" Content="DATE" FontFamily="Century Gothic" FontSize="16" Height="35"  HorizontalAlignment="Right" Name="button8" VerticalAlignment="Bottom" Click="button8_Click" Width="85" Grid.Row="1" />
