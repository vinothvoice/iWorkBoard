        <TextBox Background="#FFCCD1C7" BorderThickness="0" FontFamily="Century Gothic" FontSize="25" Height="229" HorizontalAlignment="Left" Margin="21,0,0,33" Name="textBox4" Padding="0" Text="Your Notes Here..." TextAlignment="Left" VerticalAlignment="Bottom" Width="331" MinLines="15" AcceptsReturn="True" TextWrapping="Wrap" Grid.RowSpan="2" TabIndex="9" />
        <Button Content="Expand" Height="23" HorizontalAlignment="Left" Margin="143,0,0,33" Name="button13" VerticalAlignment="Bottom" Width="75" Click="button13_Click" Background="#FFF2FFFF" Grid.Row="1" BorderBrush="#FFF2FFFF" />
