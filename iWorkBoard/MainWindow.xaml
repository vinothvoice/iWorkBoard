<Window x:Class="iWorkBoard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="iWorkBoard" WindowState="Maximized" AllowsTransparency="False" Opacity="0.9" Background="Black" WindowStyle="None" mc:Ignorable="d" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" Height="874" Width="1335" ResizeMode="NoResize" Topmost="False" Icon="/iWorkBoard;component/iWB.ico">
  
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="573*" />
            <RowDefinition Height="262*" />
