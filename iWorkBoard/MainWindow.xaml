        <Image HorizontalAlignment="Stretch" Name="image1" Stretch="UniformToFill" VerticalAlignment="Stretch" Width="Auto" Source="/iWorkBoard;component/helpF.jpg" Grid.RowSpan="2" Visibility="Hidden" ImageFailed="image1_ImageFailed" />
        <Button Background="#FF100F0F" BorderBrush="#FF3E3B39" BorderThickness="0" Content="Close Help" FontFamily="Century Gothic" FontSize="16" Height="35" HorizontalAlignment="Right" Name="button15" VerticalAlignment="Top" Width="109" Foreground="White" Click="button15_Click" Visibility="Hidden" Margin="0,12,12,0" />
    </Grid>
</Window>
