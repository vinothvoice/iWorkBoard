        <Button Background="#FFF59D23" BorderBrush="#FFE59D23" BorderThickness="0" Content="Open" FontFamily="Century Gothic" FontSize="16" Height="35" HorizontalAlignment="Left" Margin="81,0,0,0" Name="button3" VerticalAlignment="Top" Width="75" Click="button3_Click" />
        <Button Background="#FFE59D23" BorderBrush="#FFE59D23" BorderThickness="0" Content="Help" FontFamily="Century Gothic" FontSize="16" Height="35" HorizontalAlignment="Left" Name="button4" VerticalAlignment="Top" Width="75" Margin="162,0,0,0" Click="button4_Click" />
        <Rectangle HorizontalAlignment="Left" Margin="21,84,0,30" Name="rectangle1" Stroke="Gray" VerticalAlignment="Stretch" Width="331" />
